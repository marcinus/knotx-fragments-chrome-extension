{
  "name": "knotx-chrome-devtool-extension",
  "version": "2.2.1-SNAPSHOT",
  "description": "Extends the Developer Tools, adding a sidebar that displays the Knotx Fragments data associated with the selected DOM element.",
  "private": true,
  "browserslist": [
    "last 1 chrome version"
  ],
  "scripts": {
    "dev": "webpack",
    "watch": "webpack --watch",
    "test": "jest -c jest.config.js",
    "test:snapshot": "jest -c jest-snapshot.config.js",
    "snapshot": "start-server-and-test storybook http-get://localhost:6006 test:snapshot",
    "lint": "eslint -c .eslintrc.json 'src/js/**'",
    "release": "node ./scripts/build.js",
    "release:minor": "node ./scripts/build.js minor",
    "release:major": "node ./scripts/build.js major",
    "clean:artifacts": "rm -rf packages/*.zip",
    "storybook": "start-storybook -p 6006 --ci",
    "build-storybook": "build-storybook"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "keywords": [
    "knot.x",
    "integrations",
    "configurable integrations",
    "debug",
    "chrome extension"
  ],
  "author": "Knot.x Community",
  "license": "Apache License 2.0",
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addon-storyshots": "^5.3.18",
    "@storybook/addon-storysource": "^5.3.18",
    "@testing-library/jest-dom": "^5.1.1",
    "archiver": "^3.1.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.7",
    "canvas": "^2.6.1",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.3.0",
    "jest-extended": "^0.11.5",
    "jest-image-snapshot": "^3.0.1",
    "jest-junit-reporter": "^1.1.0",
    "jest-puppeteer": "^4.4.0",
    "puppeteer": "^3.0.0",
    "react-test-renderer": "^16.13.1",
    "semver": "^7.1.2",
    "start-server-and-test": "^1.11.0",
    "style-loader": "^1.1.3",
    "themeprovider-storybook": "^1.5.1",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "addon-redux": "^1.1.0",
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "redux": "^4.0.5",
    "renderjson": "^1.4.0",
    "storybook-addon-styled-component-theme": "^1.3.0",
    "styled-components": "^5.0.1",
    "unique-selector": "^0.4.1",
    "vis-network": "^7.3.4",
    "vis-timeline": "^7.1.2",
    "webext-redux": "^2.1.6"
  }
}